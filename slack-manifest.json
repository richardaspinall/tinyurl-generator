{
  "_metadata": {
    "major_version": 1,
    "minor_version": 1
  },
  "display_information": {
    "name": "tinyurl-generator"
  },
  "features": {
    "bot_user": {
      "display_name": "TinyURL Generator",
      "always_online": false
    },
    "shortcuts": [
      {
        "name": "Create a new TinyURL",
        "type": "global",
        "callback_id": "tinyurl_generator",
        "description": "Creates a new TinyURL from a given URL"
      }
    ]
  },
  "oauth_config": {
    "scopes": {
      "bot": ["commands"]
    }
  },
  "settings": {
    "interactivity": {
      "is_enabled": true,
      "request_url": "https://XXXXXXXXXX.ngrok.io/tinyurlgenerator"
    },
    "org_deploy_enabled": false,
    "socket_mode_enabled": false,
    "is_hosted": false
  }
}
